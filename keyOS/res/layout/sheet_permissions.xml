<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/list"
    android:layout_width="match_parent"
    android:background="@drawable/background_sheet"
    android:layout_height="match_parent"
    android:clipToPadding="false"
   >
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_marginHorizontal="10dp"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_marginTop="25dp"
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <androidx.cardview.widget.CardView
                android:layout_width="35dp"
                android:layout_marginLeft="10dp"
                app:cardElevation="0dp"
                android:layout_centerInParent="true"
                android:layout_alignParentLeft="true"
                app:cardCornerRadius="17.5dp"
                android:layout_height="35dp">

                <ImageView
                    android:background="#F69705"
                    app:srcCompat="@drawable/usage"








                    android:padding="10dp"
                    app:tint="@android:color/white"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="65dp"
                android:fontFamily="@font/montserrat"
                android:gravity="center_vertical"
                android:text="Usage Access"
                android:textColor="@android:color/black"
                android:textSize="16dp" />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_centerInParent="true"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content">
                <com.suke.widget.SwitchButton
                    android:id="@+id/usage"
                    app:sb_show_indicator="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <androidx.cardview.widget.CardView
                android:layout_width="35dp"
                android:layout_marginLeft="10dp"
                app:cardElevation="0dp"
                android:layout_centerInParent="true"
                android:layout_alignParentLeft="true"
                app:cardCornerRadius="17.5dp"
                android:layout_height="35dp">

                <ImageView
                    android:background="#0979F2"
                    app:srcCompat="@drawable/accessablity"
                    android:padding="10dp"
                    app:tint="@android:color/white"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:text="Accessibility"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:layout_marginLeft="65dp"
                android:textSize="16dp"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_centerInParent="true"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content">
                <com.suke.widget.SwitchButton
                    android:id="@+id/accessablity"
                    app:sb_show_indicator="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <androidx.cardview.widget.CardView
                android:layout_width="35dp"
                android:layout_marginLeft="10dp"
                app:cardElevation="0dp"
                android:layout_centerInParent="true"
                android:layout_alignParentLeft="true"
                app:cardCornerRadius="17.5dp"
                android:layout_height="35dp">

                <ImageView
                    android:background="#E91E63"
                    app:srcCompat="@drawable/overlay"
                    android:padding="10dp"
                    app:tint="@android:color/white"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:text="Overlay"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:layout_marginLeft="65dp"
                android:textSize="16dp"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_centerInParent="true"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content">
                <com.suke.widget.SwitchButton
                    android:id="@+id/overlay"
                    app:sb_show_indicator="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <androidx.cardview.widget.CardView
                android:layout_width="35dp"
                android:layout_marginLeft="10dp"
                app:cardElevation="0dp"
                android:layout_centerInParent="true"
                android:layout_alignParentLeft="true"
                app:cardCornerRadius="17.5dp"
                android:layout_height="35dp">

                <ImageView
                    android:background="#4CAF50"
                    app:srcCompat="@drawable/write_settings"
                    android:padding="10dp"
                    app:tint="@android:color/white"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:text="Write Settings"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:layout_marginLeft="65dp"
                android:textSize="16dp"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_centerInParent="true"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content">
                <com.suke.widget.SwitchButton
                    android:id="@+id/writeSettings"
                    app:sb_show_indicator="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <androidx.cardview.widget.CardView
                android:layout_width="35dp"
                android:layout_marginLeft="10dp"
                app:cardElevation="0dp"
                android:layout_centerInParent="true"
                android:layout_alignParentLeft="true"
                app:cardCornerRadius="17.5dp"
                android:layout_height="35dp">

                <ImageView
                    android:background="#03A9F4"
                    app:srcCompat="@drawable/admin"
                    android:padding="10dp"
                    app:tint="@android:color/white"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:text="Admin"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:layout_marginLeft="65dp"
                android:textSize="16dp"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_centerInParent="true"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content">
                <com.suke.widget.SwitchButton
                    android:id="@+id/admin"
                    app:sb_show_indicator="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="50dp">
            <androidx.cardview.widget.CardView
                android:layout_width="35dp"
                android:layout_marginLeft="10dp"
                app:cardElevation="0dp"
                android:layout_centerInParent="true"
                android:layout_alignParentLeft="true"
                app:cardCornerRadius="17.5dp"
                android:layout_height="35dp">

                <ImageView
                    android:background="#9C27B0"
                    app:srcCompat="@drawable/contact"
                    android:padding="10dp"
                    app:tint="@android:color/white"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </ImageView>
            </androidx.cardview.widget.CardView>
            <TextView
                android:layout_width="wrap_content"
                android:text="Other Permissions"
                android:gravity="center"
                android:textColor="@android:color/black"
                android:layout_marginLeft="65dp"
                android:textSize="16dp"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_centerInParent="true"
                android:layout_alignParentRight="true"
                android:layout_height="wrap_content">
                <com.suke.widget.SwitchButton
                    android:id="@+id/other"
                    app:sb_show_indicator="false"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
        </RelativeLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>